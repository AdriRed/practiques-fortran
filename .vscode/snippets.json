{
	// Place your snippets for FortranFreeForm here. Each snippet is defined under a snippet name and has a prefix, body and 
	// description. The prefix is what is used to trigger the snippet and the body will be expanded and inserted. Possible variables are:
	// $1, $2 for tab stops, $0 for the final cursor position, and ${1:label}, ${2:another} for placeholders. Placeholders with the 
	// same ids are connected.
	// Example:
	// "Print to console": {
	// 	"prefix": "log",
	// 	"body": [
	// 		"console.log('$1');",
	// 		"$2"
	// 	],
	// 	"description": "Log output to console"
	// }
	"Interface": {
		"prefix": "interface",
		"body": [
			"interface",
			"	  $1",
			"end interface"
		]
	},
	"Simple function": {
		"prefix": "function",
		"body": [
			"double precision function $1(x) result(retval)",
			"   implicit none",
			"	  double precision, intent(in) :: x",
			"	  retval = $2",
			"end function $1"
		]
	},
	"Double function": {
		"prefix": "function2",
		"body": [
			"double precision function $1(x, y) result(retval)",
			"   implicit none",
			"	  double precision, intent(in) :: x, y",
			"	  retval = $2",
			"end function $1"
		]
	},
	"Interface simple function": {
		"prefix": "ifunc",
		"body": [
			"double precision function $1(x) ",
			"	  implicit none",
			"	  double precision, intent(in) :: x",
			"end function $1"
		]
	},
	"Interface double function": {
		"prefix": "ifunc2",
		"body": [
			"double precision function $1(x, y) ",
			"	  implicit none",
			"	  double precision, intent(in) :: x, y",
			"end function $1"
		]
	}
}